VI_A_CPP_FILES := \
	cpp/vi/vi.cpp \

VI_A_O_FILES := \
	$(BUILD_DIR)/libs/vi/vi.o \

VI_A_CFLAGS := \

VI_A_LDFLAGS := \
	-nodefaults \
	-fp hard \
	-proc gekko \
	-linkmode moreram \

$(BUILD_DIR)/libvi.a: $(VI_A_O_FILES)
	$(LD) -xm l $(VI_A_LDFLAGS) -o $(BUILD_DIR)/libvi.a $(VI_A_O_FILES)
	$(STRIP) -d -R .dead -R .comment $(BUILD_DIR)/libvi.a

$(BUILD_DIR)/libs/vi/%.o: cpp/vi/%.cpp
	mkdir -p $(@D)
	$(CC) $(CFLAGS) $(VI_A_CFLAGS) -c -o $@ $<

